[Desktop Entry]
_Name=Print Queue Applet
_Comment=System tray icon for managing print jobs
Exec=system-config-printer-applet
Terminal=false
Type=Application
Icon=printer
NotShowIn=KDE;LXDE;Unity;GNOME;
StartupNotify=false
X-GNOME-Autostart-Delay=30
X-Ubuntu-Gettext-Domain=system-config-printer
NoDisplay=true
